# Supabase configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=service-role-key

# Financial data provider (Alpha Vantage)
ALPHAVANTAGE_API_KEY=demo
# Comma-separated list of ticker symbols to poll
MARKET_SYMBOLS=AAPL,MSFT
# Optional: override interval (Alpha Vantage allows 1,5,15,30,60min)
INTRADAY_INTERVAL=5min
# Optional: overall request timeout in seconds (default 45)
ALPHAVANTAGE_TIMEOUT=45

# Weather data provider (Open-Meteo)
OPENMETEO_LATITUDE=48.8566
OPENMETEO_LONGITUDE=2.3522
OPENMETEO_LOCATION=Paris-FR
# Comma-separated hourly metrics to request (temperature_2m always included)
OPENMETEO_HOURLY_VARS=temperature_2m,relativehumidity_2m,windspeed_10m,precipitation
# Optional request timeout in seconds (default 30)
OPENMETEO_TIMEOUT=30
